    "scripts": {
        "client-install": "cd client && yarn install",
        "server-install": "cd server && yarn install",
        "server-up": "cd server && yarn start:dev",
        "client-up": "cd client && yarn dev",
        "run:both": "concurrently \"yarn run server-up\" \"yarn run client-up\"",
        "install-all-deps": "concurrently \"yarn run server-install\" \"yarn run client-install\""
